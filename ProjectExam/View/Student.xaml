<UserControl x:Class="ProjectExam.View.Student"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectExam.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <local:AddNewUser DataContext="{Binding StudentsVM}" Margin="0,60,0,0"/>
            <local:ListOfStudentView DataContext="{Binding StudentsVM}"/>
        </StackPanel>
    </Grid>
</UserControl>
